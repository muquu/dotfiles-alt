Xcursor.theme: Vanilla-DMZ-AA
Xcursor.size: 32

Xft.dpi: 96
Xft.antialias: true
Xft.hinting: false
Xft.rgba: rgb
Xft.autohint: false
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault

URxvt*termName: screen-256color

!URxvt.perl-lib: /usr/lib/urxvt/perl
URxvt.perl-ext-common:      default,clipboard,url-select,keyboard-select,xim-onthespot
URxvt.url-select.launcher:  firefox
URxvt.url-select.underline: true
URxvt.keysym.M-u:           perl:url-select:select_next
URxvt.keysym.M-Escape:      perl:keyboard-select:activate
URxvt.keysym.M-s:           perl:keyboard-select:search
URxvt.keysym.M-c:           perl:clipboard:copy
URxvt.keysym.M-v:           perl:clipboard:paste
URxvt.keysym.M-C-v:         perl:clipboard:paste_escaped

! Input Method
!URxvt.imLocale: ja_JP.UTF-8
!URxvt*inputMethod: xim
URxvt*preeditType: OnTheSpot
!URxvt*preeditType: OverTheSpot
!URxvt.preeditType: root
!URxvt.preeditType: xim-onthespot
!URxvt.preeditType: xim-overthespot

! True transparent
URxvt*depth: 32
URxvt*background: rgba:0000/0000/0200/c800

! Fake transparency
!URxvt*transparent: true
!URxvt*shading: 20

! Fonts
URxvt*font: xft:Terminus_sazanami:pixelsize=14, xft:Terminus_sazanami\ for\ Powerline:pixelsize=14
URxvt*boldFont: xft:Terminus_sazanami:pixelsize=14:bold, xft:Terminus_sazanami\ for\ Powerline:pixelsize=14:bold

! xterm settings
XTerm*faceName: xft:Terminus_sazanami:pixelsize=14, xft:Terminus_sazanami\ for\ Powerline:pixelsize=14
XTerm*geometry: 128x70

! Scrollbar
URxvt*scrollBar:  false
URxvt*saveLines: 8000

! Terminal colors
*foreground: white
*background: black
*color0:  #2E3436
*color1:  #a40000
*color2:  #4E9A06
*color3:  #C4A000
*color4:  #3465A4
*color5:  #75507B
*color6:  #ce5c00
*color7:  #babdb9
*color8:  #555753
*color9:  #EF2929
*color10: #8AE234
*color11: #FCE94F
*color12: #729FCF
*color13: #AD7FA8
*color14: #fcaf3e
*color15: #EEEEEC


